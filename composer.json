{
    "name": "vursion/vursion",
    "description": "vursion",
    "keywords": [
        "vursion",
        "laravel"
    ],
    "homepage": "https://github.com/vursion/vursion",
    "license": "MIT",
    "type": "library",
    "authors": [
        {
            "name": "Jochen Sengier",
            "email": "jochen@celcius.be",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^5.4 || ^7.0 || ^8.0",
        "guzzlehttp/guzzle": "^5.0 || ^6.0 || ^7.0",
        "vlucas/phpdotenv": "^3.0 || ^4.0 || ^5.0"
    },
    "autoload": {
        "psr-4": {
            "Vursion\\Vursion\\": "src"
        }
    },
    "scripts": null,
    "config": {
        "sort-packages": true
    },
    "extra": {
        "laravel": {
